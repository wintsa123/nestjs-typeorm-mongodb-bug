{
  "name": "nestApi",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "copy-static-files": "node copy-static-files.js",
    "build": "nest build && yarn run copy-static-files",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env RUNNING_ENV=dev nest start",
    "dev": "cross-env RUNNING_ENV=dev nest start --watch ",
    "debug": "cross-env RUNNING_ENV=dev nest start --debug --watch ",
    "prod": "cross-env RUNNING_ENV=prod node dist/main ",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "pm2:prod": "yarn run build && cross-env RUNNING_ENV=prod pm2 start ecosystem.config.js --env production",
    "pm2:dev": "yarn run build && pm2 start ecosystem.config.js --env development",
    "pm2:test": "yarn run build && pm2 start ecosystem.config.js --env test",
    "update": "git pull &&yarn install &&yarn build &&pm2 restart zw-nestjs &&pm2 log",
    "gitpush": "git add . &&git commit -m '1' &&git push"

  },
  "dependencies": {
    "@fastify/cookie": "6.0.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/static": "^6.12.0",
    "@fddnpm/fasc-openapi-node-sdk": "^5.3.3-4",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-fastify": "^10.2.7",
    "@nestjs/platform-socket.io": "^10.2.9",
    "@nestjs/schedule": "^3.0.4",
    "@nestjs/swagger": "^7.1.14",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "^10.2.9",
    "@wintsa/wxmsgcrypt": "^1.0.6",
    "ali-oss": "^6.18.1",
    "axios": "^1.5.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "fastify-csrf": "^3.1.0",
    "fastify-swagger": "^5.2.0",
    "fastify-xml-body-parser": "^2.2.0",
    "fs-extra": "^11.1.1",
    "ioredis": "^5.3.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mysql2": "^3.6.1",
    "patch-package": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "serve-static": "^1.15.0",
    "socket.io": "^4.7.2",
    "svg-captcha": "^1.4.0",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1",
    "xml2js": "^0.6.2",
    "yaml": "^2.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.199",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "eslint --fix",
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  }
}
